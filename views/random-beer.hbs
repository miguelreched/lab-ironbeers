<div>
   {{#each this.beers}}

    <img src="{{this.image_url}}" alt="" height="200px">
    <h3>{{this.name}}</h3>
    <p>{{this.description}}</p>
    <h4>{{this.tagline}}</h4>

    <ul>
{{#each this.food_pairing}}
            <li>{{this}}</li>
{{/each}}
    </ul>

    <p>{{this.brewers_tips}}</p>

    {{/each}}

    
</div>

